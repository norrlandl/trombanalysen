// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") //"file:.dev.db"
}


model User {
  id String @id @default(uuid())
  createdAt DateTime
  company String
  role String
  firstName String?
  lastName String?
  email String?
  password String?
}
